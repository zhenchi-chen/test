#import <Foundation/Foundation.h>

@interface ClockTimeModel : NSObject
@property (nonatomic,strong)NSMutableArray *week; //数组，星期，多选
@property (nonatomic,strong)NSString *hour; //小时
@property (nonatomic,strong)NSString *min; //分钟
@property (nonatomic,strong)NSString *times; //闹钟循环次数,是否循环 30000 永久循环 0取消 1就一次
@property (nonatomic,strong)NSString *run; //1为启用该闹钟
@property (nonatomic,strong)NSString *name; //闹钟名字
@property (nonatomic,strong)NSString *ringTime;//闹钟响的时间戳,当run为1的时候并且闹钟只响一次才有用,单位是秒。

@end
